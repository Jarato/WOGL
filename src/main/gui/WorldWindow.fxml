<AnchorPane prefHeight="200.0" prefWidth="432.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.gui.WorldWindowCtrl">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <Pane fx:id="worldCanvasPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <VBox maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="100.0">
               <children>
                  <Separator halignment="RIGHT" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
