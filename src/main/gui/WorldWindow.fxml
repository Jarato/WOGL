                        </Button>
                        <Label fx:id="creatureCountLabel" prefHeight="37.0" prefWidth="100.0" text="Label">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="plantCountLabel" prefHeight="37.0" prefWidth="100.0" text="Label">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
